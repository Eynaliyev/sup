	"scripts": {
		"clean": "ionic-app-scripts clean",
		"build": "ionic-app-scripts build",
		"build:prod": "yarn run ionic-app-scripts build --prod",
		"firebase:deploy": "firebase deploy",
		"generate:sourcemaps": "node source-map-script.js",
		"ionic:build": "npm-run-all build generate:sourcemaps",
		"ionic:build:prod": "npm-run-all build:prod generate:sourcemaps",
		"ionic:build:prod:deploy": "npm-run-all ionic:build:prod firebase:deploy generate:docs",
		"generate:docs": "compodoc -p src/tsconfig.app.json --hideGenerator",
		"ionic:serve": "npm-run-all generate:sourcemaps ionic-app-scripts serve ",
		"ionic:serve:lab": "ionic-app-scripts serve --lab",
		"lint": "ionic-app-scripts lint",
		"security-check": "nsp check",
		"start": "npm-run-all ionic:serve:lab generate:sourcemaps",
		"test": "karma start ./test-config/karma.conf.js",
		"test-ci": "karma start ./test-config/karma.conf.js --single-run",
		"test-coverage": "karma start ./test-config/karma.conf.js --coverage",
		"e2e": "yarn run e2e-update && yarn run e2e-test",
		"e2e-test": "protractor ./test-config/protractor.conf.js",
		"e2e-update": "webdriver-manager update --standalone false --gecko false"
	},
